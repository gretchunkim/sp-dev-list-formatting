{
  "$schema": "https://developer.microsoft.com/json-schemas/sp/v2/view-formatting.schema.json",
  "hideSelection": true,
  "hideColumnHeader": true,
  "groupProps": {
    "headerFormatter": {
      "elmType": "div",
      "txtContent": "@group.fieldData.displayValue",
      "attributes": {
        "class": "ms-fontColor-themeDark ms-bgColor-themeLighter"
      },
      "style": {
        "border-left": "10px solid",
        "display": "flex",
        "flex-direction": "row",
        "width": "98%",
        "height": "50px",
        "margin-top": "10px",
        "margin-bottom": "10px",
        "padding-left": "20px",
        "font-size": "20px"
      }
    }
  },
  "rowFormatter": {
    "elmType": "div",
    "style": {
      "margin": "22px 0 12px 44px",
      "gap": "24px",
      "flex-wrap": "wrap"
    },
