      {
        "comm": "1",
        "elmType": "div",
        "attributes": {
          "class": "ms-bgColor-themeLighterAlt"
        },
        "style": {
          "display": "=if([$Title1],'flex','none')",
          "flex-direction": "column",
          "width": "226px",
          "height": "180px",
          "border": "1px solid #ccc",
          "border-radius": "10px",
          "box-shadow": "0 3px 10px rgba(0,0,0,.2)",
          "overflow": "hidden"
        },
        "children": [
          {
            "elmType": "a",
            "attributes": {
              "class": "ms-fontColor-themeDark ms-bgColor-themeDark",
              "href": "[$Link1]",
              "target": "_blank"
            },
            "style": {
              "height": "65%",
              "cursor": "=if([$Link1],'pointer','default')"
            },
            "children": [
              {
                "elmType": "img",
                "attributes": {
                  "src": "=getThumbnailImage([$Image1], 300, 300)"
                },
                "style": {
                  "display": "=if([$Image1],'','none')",
                  "object-fit": "cover",
                  "height": "100%",
                  "width": "100%"
                }
              }
            ]
          },
          {
            "txtContent": "[$Title1]",
            "elmType": "div",
            "attributes": {
              "class": "ms-fontWeight-bold ms-fontColor-neutralPrimary"
            },
            "style": {
              "border-top": "2px solid",
